{
  "name": "calculator",
  "version": "1.0.0",
  "repository": {
    "type": "git"
  },
  "description": "Calculator",
  "scripts": {
    "lint": "eslint --ext .jsx --ext .js src/",
    "start": "env-cmd debug nodemon --exec babel-node src/server/app.js",
    "build": "npm run lint && npm run build-server && npm run build-client",
    "build-client": "rm -rf dist/client && env-cmd buildclient webpack --progress --config src/webpack.production.js",
    "build-server": "rm -rf dist/server && mkdir -p dist/server && env-cmd buildserver babel ./src/server -s -d ./dist/server/main",
    "docker-build": "docker build -t calculator:local ."
  },
  "author": "Sam Hughes | samhughes1701@hotmail.co.uk",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "brotli-gzip-webpack-plugin": "0.5.0",
    "css-loader": "^0.28.11",
    "env-cmd": "^8.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest-each": "^24.0.0",
    "lodash-es": "^4.17.10",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "4.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "recompose": "^0.27.1",
    "rxjs": "^6.2.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-command": "^0.4.0",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.2.0",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "expr-eval": "^1.2.2",
    "express": "^4.16.3",
    "express-static-gzip": "^0.3.2"
  }
}
