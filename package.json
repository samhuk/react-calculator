{
  "name": "calculator",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://stash.customappsteam.co.uk/scm/fstdc/calculator.git"
  },
  "description": "Calculator",
  "scripts": {
    "lint": "eslint --ext .jsx --ext .js src/",
    "start": "env-cmd debug nodemon --exec babel-node src/server/app.js",
    "build": "npm run lint && npm run build-server && npm run build-client",
    "build-client": "rm -rf dist/client && env-cmd buildclient webpack --progress --config src/webpack.production.js",
    "build-server": "rm -rf dist/server && mkdir -p dist/server && env-cmd buildserver babel ./src/server -s -d ./dist/server/main && env-cmd buildserver babel ./src/shared -s -d ./dist/server/shared",
    "docker-build": "docker build -t calculator:local ."
  },
  "author": "",
  "license": "UNLICENSED",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
